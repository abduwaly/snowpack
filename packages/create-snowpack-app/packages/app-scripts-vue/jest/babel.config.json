{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@snowpack/babel-plugin-package-import",
      {
        "webModulesUrl": "../web_modules",
        "ignore": [
          "@testing-library/vue",
          "@testing-library/jest-dom/extend-expect"
        ]
      }
    ]
  ]
}
